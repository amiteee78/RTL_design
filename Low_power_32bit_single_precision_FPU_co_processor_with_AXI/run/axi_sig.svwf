	{[format {axi_slave_tb.axi_slv_lt.arready}]}
	} ]]
set id [waveform add -signals [subst  {
	{[format {axi_slave_tb.axi_slv_lt.rvalid}]}
	} ]]
set id [waveform add -signals [subst  {
	{[format {axi_slave_tb.axi_slv_lt.rready}]}
	} ]]
set id [waveform add -signals [subst  {
	{[format {axi_slave_tb.axi_slv_lt.rdata[31:0]}]}
	} ]]
set id [waveform add -signals [subst  {
	{[format {axi_slave_tb.axi_slv_lt.rresp[1:0]}]}
	} ]]

waveform xview limits 0 2465ns
